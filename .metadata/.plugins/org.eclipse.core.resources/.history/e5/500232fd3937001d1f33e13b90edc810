package arraysDemo;

public class Main {

	public static void main(String[] args) {
		
		String[] ogrenciler = new String[3];
		ogrenciler[0]="Engin";
		ogrenciler[1]="Derin";
		ogrenciler[2]="Salih";
		
		for(int i=1; i<ogrenciler.length; i++) {
			System.out.println("Öğrenci : "+ogrenciler);
		}
	}

}
