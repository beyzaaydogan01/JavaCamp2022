package sayiBulma;

public class Main {

	public static void main(String[] args) {
		int[] sayilar = new int[] {1,2,5,7,9,0};
		int aranacak = 22;
		boolean varMi = false;
		
		for(int i=0; i<sayilar.length;i++) {
			if(aranacak == sayilar[i]) {
				varMi = true;
			}
		}
		
		if(varMi) {
			System.out.println("Say覺 vard覺r : " +aranacak);
		}else {
			System.out.println("Say覺 bulunmamaktad覺r");
		}
	}

}
